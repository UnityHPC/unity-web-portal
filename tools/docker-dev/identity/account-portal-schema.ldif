dn: cn=accountPortal,cn=schema,cn=config
objectClass: olcSchemaConfig
cn: accountPortal
olcAttributeTypes: ( 1.1.1 NAME 'isDisabled'
    DESC 'if disabled, systems should pretend this group does not exist'
    EQUALITY booleanMatch
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.7
    SINGLE-VALUE
    )
olcAttributeTypes: ( 1.1.3 NAME 'managerUid'
    DESC 'users with permission to manage group'
    EQUALITY caseExactIA5Match
    SYNTAX 1.3.6.1.4.1.1466.115.121.1.26
    )
olcObjectClasses: ( 1.1.2 NAME 'piGroup' SUP top AUXILIARY
    DESC 'PI Group'
    MAY ( isDisabled $ managerUid )
    )
